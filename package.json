{
  "name": "chatter-fe-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test:local": "npx cypress run --spec \"cypress/e2e/local/**/*.cy.ts\"",
    "test:local:ci": "start-server-and-test dev http://localhost:5173 test:local",
    "test:live": "npx cypress run --spec \"cypress/e2e/live/**/*.cy.ts\"",
    "test": "npm run test:local && npm run test:live",
    "lint": "npx eslint --fix --ext .ts . && npx prettier --write .",
    "check-lint": "npx eslint --ext .ts . && npx prettier --check ."
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "eventemitter3": "^5.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-use-websocket": "^4.13.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@bouzomgi/eslint-config-base": "^1.0.8",
    "@eslint/js": "^9.30.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "chatter-be": "github:Bouzomgi/chatter-be#main",
    "cypress": "^14.5.3",
    "cypress-mock-websocket-plugin": "^2.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "sass-embedded": "^1.89.2",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
